{
  "name": "inku-kaehmy-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "dev": "webpack --mode=development",
    "build": "webpack",
    "deploy-local": "npm run build && cp dist/bundle.js ~/code/suur-inkubio/assets/bundles/kahmy.js",
    "deploy-remote": "npm run build && scp dist/bundle.js inkubio@otax.fi:~/www-data/wp-content/themes/suurinkubio/assets/bundles/kahmy.js",
    "stats": "webpack --json > stats.json"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^2.3.0",
    "autoprefixer": "7.1.6",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "3.1.1",
    "chalk": "1.1.3",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "draft-js": "0.10.5",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-dev-utils": "5.0.1",
    "react-dom": "16.4.1",
    "react-router-dom": "4.3.1",
    "resolve": "1.6.0",
    "source-map-loader": "0.2.1",
    "tinymce": "^4.8.3",
    "tslint": "5.7.0",
    "tslint-config-prettier": "1.10.0",
    "tslint-react": "3.2.0",
    "url-loader": "0.6.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/draft-js": "0.10.23",
    "@types/jest": "23.1.3",
    "@types/node": "10.5.1",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "^4.0.27",
    "@types/react-router-dom": "4.2.7",
    "@types/tinymce": "^4.5.16",
    "css-loader": "0.28.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "prettier": "1.13.7",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.2",
    "typescript": "2.9.2",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
